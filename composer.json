{
    "name": "boxed-code/laravel-scout-elasticsearch",
    "description": "Elasticsearch Driver for Laravel Scout",
    "keywords": ["laravel", "scout", "elasticsearch", "elastic", "aws", "elasticsearch service"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Green",
            "email": "oliver@boxedcode.co.uk"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/scout": "^8.0|^9.0",
        "elasticsearch/elasticsearch": "^5.0|^6.0"
    },
     "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.5|~8.5|~9.3"
    },
    "autoload": {
        "psr-4": {
            "BoxedCode\\Laravel\\Scout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": ",tests"
        }
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Required to use, the Elasticsearch engine (^5.0)."
    },
    "extra": {
        "laravel": {
            "providers": [
                "BoxedCode\\Laravel\\Scout\\ElasticsearchServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
